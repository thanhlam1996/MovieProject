<!-- Top Movies Area Start -->
<section class="filmoja-top-movies-area section_70">
	<div class="container">
		<div class="row">
			<div class="col-md-12">
				<div class="filmoja-heading">
					<h2>Top Phim :<span> 2019</span></h2>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="col-md-12">
				<div class="top-movie-slider owl-carousel">


					<%movietop.forEach((t,index)=>{%>
					<%if(index<5){%>
					<%var rat=0, rating=0%>
					<%var countrat=0%>
					<%t.ratings.forEach((r)=>{%>
					<%rat+=parseFloat(r);%>
					<%countrat++%>
					<%})%>
					<%if(countrat==0){%>
					<%countrat=1;%>
					<%}%>
					<%var num=(rat/countrat).toFixed(1)%>
					<%if((num*10)%10==0){%>
					<%rating=(rat/countrat).toFixed(0)%>
					<%}else{%>
					<%rating=(rat/countrat).toFixed(1)%>
					<%}%>
											<div class="single-top-movie">
													<div class="top-movie-wrap">
														<div class="top-movie-img">
															<a href="#">
															<img src="https://s3-us-west-2.amazonaws.com/movies-project-bucket/<%=t.info.posterimage%>" alt="top movies" />
															</a>
														</div>
														<div class="thumb-hover">
															<a class="play-video" href="<%=t.info.trailer%>"><i class="fa fa-play"></i></a>
															<div class="thumb-date">
																<span>05:30</span>
															</div>
														</div>
													</div>
													<div class="top-movie-details">
														<h4><a href="/detail-movie?id=<%=t.id%>"><%=t.title%></a></h4>
					<div class="movie-details-thumbs">
						<ul>
							<li>
								<a href="#"><i class="fa fa-thumbs-up"></i>
									<%=t.countlike%></a>
							</li>
							<li>
								<a href="#"><i class="fa fa-eye"></i><%=t.movieview%></a>
							</li>
							<li>
								<a href="#"><span class="rating"><i class="fa fa-star"></i><%=rating%></span></a>
							</li>
						</ul>
					</div>
				</div>

			</div>
			<%}else{%>
			<%return%>
			<%}%>		
												
									<%})%>




		</div>
	</div>
	</div>
	</div>
</section>



