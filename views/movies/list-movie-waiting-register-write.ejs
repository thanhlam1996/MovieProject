<%include ../partials/header-admin%>

<!-----------------------------------------------Container ------------------------------------------------>
<%var count=1%>
<div class="container lst-movie-registed-get">
  
    <%if(role<=2){%>
    <%result.Items.forEach(function(i){%>
    <%if(i.stt==4){%>
    <div class="item-registerlist btnregistionmovie<%=count%>" id="<%=i.id%>">
        <div class="row registerlist-title">
            <label class="col col-1">
                <h3><%=count%></h3>
            </label>
            <label class="col col-11">
                <h3 class="<%=i.id%>" id="<%=i.title%>"><%=i.title%></h3>
            </label>
        </div>
        <div class="row registerlist-label-topic">
            <label class="col col-2">Nhà phát hành</label>
            <label class="col col-2">Người tạo</label>
            <label class="col col-2">Hạn đăng ký</label>
            <label class="col col-3">Ghi chú</label>
            <div class="col col-3">
                <button class="btn btn-success btnregistionmovie" id="btnregistionmovie<%=count%>">Đăng ký</button>
            </div>
        </div>
        <div class="row registerlist-detail">
            <label class="col col-2"><%=i.info.producer%></label>
            <label class="col col-2"><%=i.process.create.creater[0]%></label>
            <label class="col col-2 deadline"><%=i.process.create.deadline%></label>
            <label class="col col-3"><%=i.process.create.createnote?i.process.create.createnote:"Note:"%></label>
        </div>
    </div>
    <%count++ %>
    <%}%>
            <%})%>
    <%}else{%>
    <%result.Items.forEach(function(i){%>
    <%if(i.stt==4){%>
    <div class="item-registerlist btnregistionmovie<%=count%>" id="<%=i.id%>">
        <div class="row registerlist-title">
            <label class="col col-1">
                <h3><%=count%></h3>
            </label>
            <label class="col col-11">
                <h3 class="<%=i.id%>" id="<%=i.title%>"><%=i.title%></h3>
            </label>
        </div>
        <div class="row registerlist-label-topic">
            <label class="col col-2">Nhà phát hành</label>
            <label class="col col-2">Người tạo</label>
            <label class="col col-2">Hạn đăng ký</label>
            <label class="col col-3">Ghi chú</label>
            <div class="col col-3">
                <button class="btn btn-success btnregistionmovie" id="btnregistionmovie<%=count%>">Đăng ký</button>
            </div>
        </div>
        <div class="row registerlist-detail">
            <label class="col col-2"><%=i.info.producer%></label>
            <label class="col col-2"><%=i.process.create.creater[0]%></label>
            <label class="col col-2 deadline"><%=moment(i.process.create.deadline,'YYYY-MM-DD').format('DD/MM/YYYY')%></label>
            <label class="col col-3"><%=i.process.create.createnote?i.process.create.createnote:"Note:"%></label>
            <div class="col col-3">
                <button type="button" class="btn btn-primary btn-edit-movie-waiting" id="<%=i.id%>">Sửa</button>
                <button type="button" id="<%=i.id%>" class="btn btn-danger btn-delete-movie-waiting" data-toggle="modal" data-target="#myModal"
                    id="">Xóa</button>
            </div>
        </div>
    </div>
    <%count++ %>
    <%}else{%>
    <div class="item-registerlist btnregistionmovie<%=count%>" id="<%=i.id%>">
        <div class="row registerlist-title">
            <label class="col col-1">
                <h3><%=count%></h3>
            </label>
            <label class="col col-11">
                <h3 class="<%=i.id%>" id="<%=i.title%>"><%=i.title%></h3>
            </label>
        </div>
        <div class="row registerlist-label-topic">
            <label class="col col-2">Nhà phát hành</label>
            <label class="col col-2">Người tạo</label>
            <label class="col col-2">Hạn đăng ký</label>
            <label class="col col-2">Ghi chú</label>
            <label class="col col-2">Người đăng ký</label>
            <div class="col col-2">
                <button disabled class="btn btn-success btnregistionmovie" id="btnregistionmovie<%=count%>">Đã đăng
                    ký</button>
            </div>
        </div>
        <div class="row registerlist-detail">
            <label class="col col-2"><%=i.info.producer%></label>
            <label class="col col-2"><%=i.process.create.creater[0]%></label>
            <label class="col col-2 deadline"><%=moment(i.process.create.deadline,'YYYY-MM-DD').format('DD/MM/YYYY')%></label>
            <label class="col col-2"><%=i.process.create.createnote?i.process.create.createnote:"Note:"%></label>
            <label class="col col-2"><%=i.process.registion.register[0]%></label>
            <div class="col col-2">
                <button type="button" class="btn btn-primary btn-edit-movie-waiting" id="<%=i.id%>">Sửa</button>
                <button type="button" class="btn btn-danger btn-delete-movie-waiting" id="<%=i.id%>" data-toggle="modal" data-target="#myModal"
                    id="">Xóa</button>
            </div>
        </div>
    </div>
    <%count++ %>
    <%}%>
        <%})%>
    <%}%>

       <!-- The Modal -->
  <div class="modal" id="myModal">
        <div class="modal-dialog">
          <div class="modal-content">
          
            <!-- Modal Header -->
            <div class="modal-header">
                    <h4 class="modal-title">Xác nhận</h4>
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                </div>

                <!-- Modal body -->
                <div class="modal-body">
                    Bạn có thật sự muốn xóa phim này?
                </div>

                <!-- Modal footer -->
                <div class="modal-footer">
                    <button type="button" id="btn-delete-movie-waiting" class="btn btn-success btn-remove-admin">Xong</button>
                    <button type="button" class="btn btn-primary" data-dismiss="modal">Hủy</button>
                </div>

            
          </div>
        </div>
      </div>
      
    
</div>

        <!-- --------------------------------------------End Detail--------------------------------------------->
        <%include ../partials/footer-admin%>